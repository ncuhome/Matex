  "repository": {
    "type": "git",
    "url": "https://github.com/ncuhome/Matex.git"
  },
  "scripts": {
    "electron": "electron .",
    "dev": "node ./scripts/watch.js",
    "dev:react": "vite -c ./renderer/vite.config.ts",
    "codgen": "npx playwright codegen http://localhost:3000/",
    "test:playwright": "ts-node test/preview.test.ts",
    "injection": "node ./scripts/injection.js",
    "build": "concurrently \"npm run build:react\" \"npm run build:main_preload\"",
    "build:react": "vite build --config renderer/vite.config.ts",
    "build:main_preload": "cross-env NODE_ENV=production node ./scripts/esbuild.prod.js",
    "test": "yarn test:playwright",
    "preview": "vite preview -c ./renderer/vite.config.ts",
    "postinstall": "electron-builder install-app-deps",
    "copy": "node scripts/copyFile.js",
    "prerelease": "npm run copy",
    "release": "cross-env NODE_ENV=production USE_HARD_LINKS=false electron-builder build --config electron_builder.json --publish never",
    "uploadZip": "node ./scripts/generateZip.js"
  },
  "devDependencies": {
