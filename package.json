{
  "name": "matex",
  "version": "1.2.0",
  "description": "a toolbox app",
  "private": true,
  "author": "lqd1434",
  "license": "ISC",
  "main": "./.dev/main/main.js",
  "keywords": [
    "electron",
    "Matex-app"
