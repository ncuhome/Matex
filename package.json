{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "a music app",
  "keywords": [
    "electron"
  ],
  "author": "lqd1434",
  "license": "ISC",
  "homepage": ".",
  "main": "./release/build/main/main.js",
  "build": {
    "productName": "ulisten",
    "appId": "org.ulisten.com",
    "asar": true,
    "files": [
      "electron",
      "release/build",
      "package.json"
    ],
    "mac": {
      "target": {
        "target": "default"
      },
      "hardenedRuntime": true
    },
    "directories": {
      "output": "release/app"
    }
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run remove\" \"npm run dev:electron\" \"npm run watch:electron\"",
    "dev:react": "vite",
    "remove": "rm -R ./electron/dev",
    "dev:electron": "parcel watch ./electron/main.ts --out-dir ./dev --target electron",
    "watch:electron": "wait-on ./dev/main.js && cross-env NODE_ENV=development nodemon --exec \"electron ./dev/main.js\"",
    "test:build": "electron release/build/main/main.js",
    "build": "concurrently \"npm run build:react\" \"npm run build:electron\"",
    "build:react": "tsc && vite build",
    "build:electron": "cross-env NODE_ENV=production parcel build ./electron/main.ts --out-dir ./release/build/main --target electron",
    "serve": "vite preview",
    "package": "cross-env NODE_ENV=production electron-builder build --publish never"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/babel__core": "^7.1.16",
    "@types/color-string": "^1.5.0",
    "@types/history": "^4.7.9",
    "@types/node": "^16.11.7",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/shelljs": "^0.8.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "chalk": "3.0.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "electron-debug": "^3.2.0",
    "electron-rebuild": "^3.2.5",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "glob": "^7.2.0",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.3.7",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.7.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "clsx": "^1.1.1",
    "execa": "^5.1.1",
    "fastify": "^3.24.0",
    "fastify-cli": "^2.13.0",
    "framer-motion": "^4",
    "history": "^5.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "redis": "^4.0.0-rc.4",
    "shelljs": "^0.8.4"
  }
}
