{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "a music app",
  "private": true,
  "author": "lqd1434",
  "license": "ISC",
  "homepage": ".",
  "main": "./release/build/main/main.js",
  "keywords": [
    "electron"
  ],
  "build": {
    "productName": "ulisten",
    "appId": "org.ulisten.com",
    "icon": "./electron/assets/icon/icon.png",
    "asar": true,
    "files": [
      "electron",
      "release/build",
      "package.json"
    ],
    "mac": {
      "icon": "./electron/assets/icon/icon.png",
      "hardenedRuntime": true
    },
    "directories": {
      "output": "release/app"
    }
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run remove\" \"npm run dev:electron\" \"npm run watch:electron\"",
    "dev:react": "vite",
    "dev:server": "nodemon --config nodemon-express.json",
    "remove": "rm -R ./dev",
    "dev:electron": "parcel watch ./electron/main.ts --out-dir ./dev --target electron",
    "watch:electron": "wait-on ./dev/main.js && nodemon --config nodemon-electron.json",
    "test:build": "electron release/build/main/main.js",
    "build": "concurrently \"npm run build:react\" \"npm run build:electron\"",
    "build:react": "vite build",
    "build:worker": "rollup ./src/useWorker.ts --file bundle.js --format iife",
    "build:server": "ts-node ./scripts/buildServer.ts",
    "build:electron": "cross-env NODE_ENV=production parcel build ./electron/main.ts --out-dir ./release/build/main --target electron && cp -r ./electron/scripts/preload.js ./release/build/main/",
    "serve": "vite preview",
    "package": "cross-env NODE_ENV=production electron-builder build --publish never"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/body-parser": "^1.19.2",
    "@types/color-string": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
    "@types/node": "^16.11.7",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@types/shelljs": "^0.8.9",
    "@types/signale": "^1.4.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "chalk": "3.0.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "electron-debug": "^3.2.0",
    "electron-rebuild": "^3.2.5",
    "electron-reload": "^2.0.0-alpha.1",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "glob": "^7.2.0",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.3.7",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "sass": "^1.42.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.6.14",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@material-ui/core": "^5.0.0-beta.5",
    "@material-ui/icons": "^5.0.0-beta.5",
    "@material-ui/styles": "^4.11.4",
    "ahooks": "^2.10.12",
    "better-sqlite3": "^7.4.5",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "execa": "^5.1.1",
    "express": "^4.17.1",
    "history": "^5.1.0",
    "monaco-editor": "^0.30.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "redis": "^4.0.0-rc.4",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "signale": "^1.4.0",
    "typeorm": "^0.2.41",
    "zustand": "^3.6.5"
  }
}
