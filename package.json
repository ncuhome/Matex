{
  "name": "electron-react-ts",
  "version": "1.0.0",
  "description": "00",
  "keywords": [
    "electron"
  ],
  "author": "lqd1434",
  "license": "ISC",
  "homepage": ".",
  "main": "./electron/build/main/main.js",
  "build": {
    "productName": "ElectronReact",
    "appId": "org.erb.ElectronReact",
    "asar": true,
    "files": [
      "electron",
      "package.json"
    ],
    "mac": {
      "target": {
        "target": "default"
      },
      "hardenedRuntime": true
    },
    "directories": {
      "output": "release"
    }
  },
  "scripts": {
    "dev": "concurrently -k \"npm run dev:react\" \"tsc -w ./electron/main.ts ./electron/scripts/preload.ts\"",
    "dev:electron": "cross-env NODE_ENV=development nodemon --exec \"electron ./electron/main.js\"",
    "dev:react": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "build:react": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "build:electron": "cross-env NODE_ENV=production webpack --config config/webpack.electron.js",
    "package": "cross-env NODE_ENV=production electron-builder build --publish never"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.1",
    "@types/react": "17.0.21",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-loader": "^8.2.3",
    "chalk": "3.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "electron": "^15.3.1",
    "electron-builder": "^22.13.1",
    "electron-debug": "^3.2.0",
    "electron-reload": "^2.0.0-alpha.1",
    "esbuild-loader": "^2.15.1",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "glob": "^7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.3.0",
    "nodemon": "^2.0.15",
    "postcss": "^8.3.7",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "purgecss-webpack-plugin": "^4.0.3",
    "react-refresh": "^0.10.0",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "wait-on": "^6.0.0",
    "webpack": "^5.64.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2"
  }
}
